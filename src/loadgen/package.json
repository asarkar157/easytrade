{
  "name": "easytrade-load-generator",
  "version": "0.1.0",
  "description": "Load generator for easyTrade based on loadgen-core",
  "main": "./build/src/main.js",
  "scripts": {
    "build": "rm -rf ./build & tsc",
    "start": "node .",
    "dev": "node -r ts-node/register --env-file=.env ./src/main.ts",
    "format": "prettier -w .",
    "lint": "eslint ./src"
  },
  "dependencies": {
    "@demoability/loadgen-core": "^1.3.32",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.52.1",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.56.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.56.0",
    "@opentelemetry/instrumentation": "^0.56.0",
    "@opentelemetry/resources": "^1.29.0",
    "@opentelemetry/sdk-metrics": "^1.29.0",
    "@opentelemetry/sdk-node": "^0.56.0",
    "@opentelemetry/sdk-trace-node": "^1.29.0",
    "@opentelemetry/semantic-conventions": "^1.29.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
